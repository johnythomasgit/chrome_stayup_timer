{
  "name": "Stay UP alarm",
  "description": "UP, Up, Up alarm",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "storage","tts"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Stay UP",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "clock.png",
    "32": "clock.png",
    "48": "clock.png",
    "128": "clock.png"
  }
  
}
